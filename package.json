{
  "name": "drupal-js-sdk",
  "version": "0.0.2",
  "description": "Drupal Javascript SDK",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "npm run lint-and-fix",
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "docs": "typedoc --out docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/d34dman/drupal-js-sdk.git"
  },
  "keywords": [
    "Drupal",
    "javascript",
    "api"
  ],
  "author": "D34dMan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/d34dman/drupal-js-sdk/issues"
  },
  "homepage": "https://github.com/d34dman/drupal-js-sdk#readme",
  "devDependencies": {
    "@shopify/eslint-plugin": "^40.2.1",
    "@types/jest": "^25.2.3",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "axios": "^0.21.1",
    "axios-mock-adapter": "^1.19.0",
    "codecov": "3.7.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.24.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "jest": "^25.5.4",
    "ts-jest": "^25.5.1",
    "typedoc": "^0.20.35",
    "typescript": "^3.9.5"
  }
}
