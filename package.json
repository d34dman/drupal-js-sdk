{
  "name": "drupal-js-sdk",
  "version": "0.1.0",
  "description": "Drupal JavaScript SDK",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "npm run lint-and-fix",
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "docs:typedoc:build": "typedoc --options typedoc.json --out api --theme ./node_modules/typedoc-neo-theme/bin/default",
    "docs": "npm run docs:build",
    "docs:better-docs": "jsdoc -c jsdoc.json --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/d34dman/drupal-js-sdk.git"
  },
  "keywords": [
    "Drupal",
    "javascript",
    "api"
  ],
  "author": "D34dMan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/d34dman/drupal-js-sdk/issues"
  },
  "homepage": "https://github.com/d34dman/drupal-js-sdk#readme",
  "devDependencies": {
    "@shopify/eslint-plugin": "^40.2.1",
    "@types/jest": "^25.2.3",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "@vuepress/plugin-docsearch": "^2.0.0-beta.15",
    "@vuepress/plugin-search": "^2.0.0-beta.16",
    "axios": "^0.21.1",
    "axios-mock-adapter": "^1.19.0",
    "codecov": "^3.7.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.27.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "jest": "^25.5.4",
    "jest-fetch-mock": "^3.0.3",
    "node-fetch": "^2.6.1",
    "ts-jest": "^25.5.1",
    "typedoc": "^0.20.35",
    "typedoc-neo-theme": "^1.1.1",
    "typescript": "^3.9.5",
    "vuepress": "^2.0.0-beta.16"
  },
  "dependencies": {
    "whatwg-fetch": "^3.6.2"
  }
}
