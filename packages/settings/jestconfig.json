{
  "transform": {
    "^.+\\.(t|j)sx?$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@drupal-js-sdk/(.*)$": "<rootDir>/../$1/src"
  },
  "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
  "collectCoverage": true
}