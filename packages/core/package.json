{
  "name": "@drupal-js-sdk/core",
  "version": "0.3.7",
  "description": "Drupal JavaScript SDK : Core",
  "keywords": [
    "Drupal",
    "javascript",
    "api"
  ],
  "homepage": "https://github.com/d34dman/drupal-js-sdk/tree/main/packages/core#readme",
  "bugs": {
    "url": "https://github.com/d34dman/drupal-js-sdk/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/d34dman/drupal-js-sdk.git"
  },
  "license": "ISC",
  "author": "d34dman <shibinkidd@gmail.com>",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "docs": "yarn run docs:build",
    "docs:better-docs": "jsdoc -c jsdoc.json --verbose",
    "docs:build": "vuepress build docs",
    "docs:dev": "vuepress dev docs",
    "docs:typedoc:build": "typedoc --options typedoc.json --out api --theme ./node_modules/typedoc-neo-theme/bin/default",
    "format": "yarn run lint-and-fix",
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix",
    "prepare": "yarn run build",
    "prepublishOnly": "yarn test && yarn run test-build && yarn run lint",
    "test": "jest --config jestconfig.json",
    "test-build": "yarn run build && node test-build.js",
    "tsc": "tsc",
    "preversion": "yarn run lint",
    "version": "yarn run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "axios": "^0.24.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.21.0",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.7",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "axios": "^0.24.0",
    "axios-mock-adapter": "^1.20.0",
    "codecov": "^3.8.3",
    "commitlint-plugin-workspace-scopes": "^1.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.6.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "husky": "^7.0.4",
    "jest": "^27.4.5",
    "siroc": "^0.16.0",
    "ts-jest": "^27.1.2",
    "typedoc": "^0.22.10",
    "typescript": "^4.5.4"
  }
}
