{
  "name": "@drupal-js-sdk/core",
  "version": "0.3.1",
  "description": "Drupal JavaScript SDK : Core",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "tsc": "tsc",
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "yarn run lint-and-fix",
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix",
    "prepare": "yarn run build",
    "prepublishOnly": "yarn test && yarn run lint",
    "preversion": "yarn run lint",
    "version": "yarn run format && git add -A src",
    "postversion": "git push && git push --tags",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "docs:typedoc:build": "typedoc --options typedoc.json --out api --theme ./node_modules/typedoc-neo-theme/bin/default",
    "docs": "yarn run docs:build",
    "docs:better-docs": "jsdoc -c jsdoc.json --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/d34dman/drupal-js-sdk.git"
  },
  "keywords": [
    "Drupal",
    "javascript",
    "api"
  ],
  "author": "D34dMan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/d34dman/drupal-js-sdk/issues"
  },
  "homepage": "https://github.com/d34dman/drupal-js-sdk#readme",
  "dependencies": {
    "axios": "^0.24.0"
  },
  "devDependencies": {
    "axios-mock-adapter": "^1.20.0"
  }
}
